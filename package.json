{
  "name": "@julienbenac/ally-gerermesaffaires",
  "description": "GererMesAffaires driver for AdonisJS Ally",
  "version": "1.0.3",
  "engines": {
    "node": ">=20.17.0"
  },
  "type": "module",
  "files": [
    "build/src",
    "build/stubs",
    "build/index.js",
    "build/index.d.ts",
    "build/configure.js",
    "build/configure.d.ts"
  ],
  "exports": {
    ".": {
      "import": "./build/index.js",
      "types": "./build/index.d.ts"
    },
    "./types": {
      "import": "./build/types/main.js",
      "types": "./build/types/main.d.ts"
    }
  },
  "scripts": {
    "clean": "del-cli build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "compile": "npm run clean && tsc",
    "prebuild": "npm run format && npm run lint:fix",
    "build": "npm run compile",
    "prepublishOnly": "npm run build",
    "release": "npx release-it"
  },
  "homepage": "https://github.com/julienbenac/ally-gerermesaffaires#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/julienbenac/ally-gerermesaffaires.git"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.8.2",
    "@adonisjs/eslint-config": "^2.0.0-beta.7",
    "@adonisjs/prettier-config": "^1.4.0",
    "@adonisjs/tsconfig": "^1.4.0",
    "@swc/core": "^1.9.3",
    "@types/node": "^22.10.0",
    "del-cli": "^6.0.0",
    "eslint": "^9.15.0",
    "prettier": "^3.4.0",
    "release-it": "^17.10.0",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@poppinss/utils": "^6.8.3"
  },
  "peerDependencies": {
    "@adonisjs/ally": "^5.0.2",
    "@adonisjs/core": "^6.14.1"
  },
  "author": "Julien Benac <contact@julienbenac.fr>",
  "license": "MIT",
  "keywords": [
    "adonisjs",
    "ally",
    "gerermesaffaires",
    "oauth2",
    "openid"
  ],
  "prettier": "@adonisjs/prettier-config",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore(release): ${version}",
      "tagAnnotation": "v${version}",
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}",
      "web": true
    }
  }
}
